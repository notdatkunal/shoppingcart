<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SaveMerchantAndCustomer</servlet-name>
  	<display-name>SaveMerchantAndCustomer</display-name>
  	<description></description>
  	<servlet-class>com.shoppingcart.controller.SaveMerchantAndCustomer</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SaveAdmin</servlet-name>
  	<display-name>SaveAdmin</display-name>
  	<description></description>
  	<servlet-class>com.shoppingcart.controller.SaveAdmin</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddAdmin</servlet-name>
  	<display-name>AddAdmin</display-name>
  	<description></description>
  	<servlet-class>com.shoppingcart.controller.AddAdmin</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>SaveMerchantAndCustomer</servlet-name>
  	<url-pattern>/SaveMerchantAndCustomer</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SaveAdmin</servlet-name>
  	<url-pattern>/SaveAdmin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddAdmin</servlet-name>
  	<url-pattern>/AddAdmin</url-pattern>
  </servlet-mapping>
  
</web-app>
